{
    "name": "blogdaren/phpcreeper",
    "type": "library",
    "keywords": ["spider", "crawler", "multi-process", "event-loop", "asynchronous"],
    "homepage": "http://www.phpcreeper.com",
    "description": "A new generation of multi-process asynchronous event-driven spider engine based on Workerman",
    "license": "Apache 2.0", 
    "authors": [
        {   
            "name": "blogdaren",
            "email": "blogdaren@163.com"
        }   
    ],
    "support": {
        "wiki": "http://www.phpcreeper.com/docs/",
        "source": "https://github.com/blogdaren/PHPCreeper"
    },
    "require": {
        "php": ">=7.0.0",
        "workerman/workerman": "^3.5",
        "blogdaren/configurator": "^1.0",
        "blogdaren/logger": "^1.1.2",
        "ramsey/uuid": "^3.8",
        "guzzlehttp/guzzle": "^6.4"
    },
    "suggest": {
        "ext-event": "For better performance"
    },
    "autoload": {
        "psr-4": {
            "PHPCreeper\\" : "src/"
        }   
    },  
    "minimum-stability": "dev"
